q-dialog(v-model="dialogActivity" maximized persistent)
    q-card.activitycard
            iframe(v-if="dialogActivity" :src="'activities/'+loadgame+'/index.html'" ref="activity" @load="activityFN" )

            
            q-dialog(v-model="dialogFinish" position="bottom" seamless)
                q-card.bg-primary
                    
                    q-card-section.text-center.q-pb-xs(v-if="!retryGame")
                        div
                            span.q-pa-sm.bg-white.rounded-borders.text-body1 ID de participación:
                                q-badge(:label="idparticipacion" color="primary")
                        .q-ma-xs.q-mt-md: q-btn(label="Terminar" push color="dark" no-caps type="a" href="https://homedepot.com.mx/" target="_parent")
                    
                    q-card-section.text-center.q-pb-xs(v-if="retryGame")
                        div
                            div.rounded-borders.text-body1.text-white ¿Ocurrió un error?
                        .q-ma-xs.q-my-md: q-btn(label="Participa nuevamente" push color="dark" no-caps @click="retryGameFN()")